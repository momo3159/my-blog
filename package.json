{
  "name": "my-blog",
  "version": "1.0.0",
  "private": true,
  "description": "my-blog",
  "author": "tkawakita",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
